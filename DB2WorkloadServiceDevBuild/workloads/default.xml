<?xml version="1.0" encoding="UTF-8"?>

<project name="Build Workload Default" default="build_workload_default">

	<import file="../common/buildWorkload.xml"/>
	
	<!-- ==================================
		Builds the default workload collection
		================================== -->
	<target name="build_workload_default">
		
		<antcall target="build_workload" inheritall="true">
			<param name="wkl.project.name" value="Workload_DefaultCollection"/>
			<param name="build.target" value="${build.target}"/>
		</antcall>
		
	</target>
</project>
